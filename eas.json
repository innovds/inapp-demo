{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "channel": "development",
      "node": "20.11.0",
      "pnpm": "9.0.0"
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      },
      "autoIncrement": true,
      "channel": "preview",
      "node": "20.11.0",
      "pnpm": "9.0.0"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "node": "20.11.0",
      "pnpm": "9.0.0"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6752774688"
      },
      "android": {
        "serviceAccountKeyPath": "./gps-sa.json",
        "track": "internal"
      }
    }
  }
}
