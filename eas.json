{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "channel": "development"
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "innov-ds@gmail.com",
        "ascAppId": "6752774688",
        "appleTeamId": "4342cde6-1fa2-4c64-a63b-171275f0dc36"
      },
      "android": {
        "serviceAccountKeyPath": "./gps-sa.json",
        "track": "internal"
      }
    }
  }
}